   "source": [
    "# https://www.acmicpc.net/problem/2839  # 어렵\n",
    "\n",
    "import sys\n",
    "\n",
    "input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "cnt = 0\n",
    "\n",
    "while N>=0:\n",
    "    if N % 5 ==0:\n",
    "        cnt += N//5\n",
    "        print(cnt)\n",
    "        break\n",
    "    N -= 3\n",
    "    cnt += 1\n",
    "\n",
    "else:\n",
    "    print(-1)\n"
   ]
  }
